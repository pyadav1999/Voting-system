@model Voting_system.Models.image
@{
    ViewBag.Title = "Home Page";
}
<script src="https://code.jquery.com/jquery-3.5.1.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        function newtab({
            
        });
    });
</script>
<div class="jumbotron">
    <h1>Voting System</h1>
    <p class="lead">Welcome To Voting System.</p>
    <p><a href="@Url.Action("Index","Voting")" class="btn btn-primary btn-lg">Start Voting</a></p>
</div>

<div class="container">
    @using (Html.BeginForm("uploadimg", "Voting", FormMethod.Post, new { @enctype = "multipart/form-data" }))
    {

        <div class="block">
            <div class="img-upload">
                
                <input type="file" Id="file-img" accept="image/png, image/gif, image/jpeg" name="file" /><br />
                <button id="upload" class="btn btn-primary" onclick="window.open('@Url.Action("Display","Voting")')">Upload Image</button>
            </div>
        </div>
    }
</div>
